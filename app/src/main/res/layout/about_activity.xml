                        android:textSize="@dimen/f4"
                        android:textColor="#177cb0"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_width="match_parent"

                        android:layout_height="25dp"
                />
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:gravity="center_vertical"
                        android:id="@+id/about_os_qn"
                        android:layout_height="match_parent">
                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content">
                        <TextView
                                android:text="QNotified"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:textColor="#ff000000"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f5"
                        />
                        <TextView
                                android:text="Copyright (C) 2019-2021 ferredoxin"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                        <TextView
                                android:text="AGPL-3.0 License with EULA"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                    </LinearLayout>
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:background="@mipmap/icon_arrow_right"
                    />
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:id="@+id/about_os_xposed"
                        android:layout_height="match_parent">

                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content">
                        <TextView
                                android:text="Xposed"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:textColor="#ff000000"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f5"
                        />
                        <TextView
                                android:text="Copyright 2013 rovo89, Tungstwenty"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                        <TextView
                                android:text="Apache License 2.0"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                    </LinearLayout>
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:background="@mipmap/icon_arrow_right"
                    />
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:id="@+id/about_os_mmkv"
                        android:layout_height="match_parent">

                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content">
                        <TextView
                                android:text="MMKV"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:textColor="#ff000000"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f5"
                        />
                        <TextView
                                android:text="Copyright (C) 2018 THL A29 Limited, a Tencent company"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                        <TextView
                                android:text="BSD 3-Clause License"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                    </LinearLayout>
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:background="@mipmap/icon_arrow_right"
                    />
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:id="@+id/about_os_xa"
                        android:layout_height="match_parent">

                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content">
                        <TextView
                                android:text="XAutoDaily"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:textColor="#ff000000"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f5"
                        />
                        <TextView
                                android:text="Copyright (C) 2022, teble"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                        <TextView
                                android:text="LGPL-3.0 License"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                    </LinearLayout>
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:background="@mipmap/icon_arrow_right"
                    />
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical"
                        android:layout_marginRight="10dp"
                        android:layout_marginBottom="5dp"
                        android:id="@+id/about_os_dexkit"
                        android:layout_height="match_parent">

                    <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="wrap_content">
                        <TextView
                                android:text="DexKit"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:textColor="#ff000000"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f5"
                        />
                        <TextView
                                android:text="Copyright (C) 2022, teble"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                        <TextView
                                android:text="GPL-3.0 License"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:layout_gravity="left"
                                android:textSize="@dimen/f6"
                        />
                    </LinearLayout>
                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:background="@mipmap/icon_arrow_right"
                    />
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
    </ScrollView>
</RelativeLayout>
