                    <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="right"
                            android:background="@mipmap/icon_arrow_right"
                    />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
